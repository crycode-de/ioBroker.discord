{
  "name": "iobroker.discord",
  "version": "2.1.2",
  "description": "ioBroker Discord bot integration",
  "author": {
    "name": "Peter MÃ¼ller",
    "email": "peter@crycode.de",
    "url": "https://crycode.de"
  },
  "homepage": "https://github.com/crycode-de/ioBroker.discord",
  "license": "MIT",
  "keywords": [
    "discord",
    "notification",
    "message",
    "bot",
    "iobroker"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/crycode-de/ioBroker.discord"
  },
  "engines": {
    "node": ">=16.9.0",
    "npm": ">=7.0.0"
  },
  "dependencies": {
    "@iobroker/adapter-core": "^3.0.3",
    "autobind-decorator": "^2.4.0",
    "discord.js": "^14.13.0",
    "source-map-support": "^0.5.21"
  },
  "devDependencies": {
    "@alcalzone/release-script": "^3.6.0",
    "@alcalzone/release-script-plugin-iobroker": "^3.6.0",
    "@alcalzone/release-script-plugin-license": "^3.5.9",
    "@alcalzone/release-script-plugin-manual-review": "^3.5.9",
    "@iobroker/adapter-dev": "^1.2.0",
    "@iobroker/testing": "^4.1.0",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/mocha": "^10.0.1",
    "@types/node": "^18.17.14",
    "@types/proxyquire": "^1.3.28",
    "@types/sinon": "^10.0.16",
    "@types/sinon-chai": "^3.2.9",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "chai": "^4.3.8",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.48.0",
    "mocha": "^10.2.0",
    "proxyquire": "^2.1.3",
    "rimraf": "^5.0.1",
    "sinon": "^15.2.0",
    "sinon-chai": "^3.7.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2"
  },
  "main": "build/main.js",
  "files": [
    "admin{,/!(src)/**}/!(tsconfig|tsconfig.*).json",
    "admin{,/!(src)/**}/*.{html,css,png,svg,jpg,js}",
    "build/",
    "io-package.json",
    "LICENSE"
  ],
  "scripts": {
    "prebuild": "rimraf build admin/build",
    "build": "build-adapter ts",
    "watch": "build-adapter ts --watch",
    "prebuild:ts": "rimraf build",
    "build:ts": "build-adapter ts",
    "watch:ts": "build-adapter ts --watch",
    "test:ts": "mocha --config test/mocharc.custom.json src/**/*.test.ts",
    "test:package": "mocha test/package --exit",
    "test:unit": "mocha test/unit --exit",
    "test:integration": "mocha test/integration --exit",
    "test": "npm run test:ts && npm run test:package",
    "check": "tsc --noEmit",
    "lint": "eslint --ext .ts src/",
    "translate": "translate-adapter -b admin/i18n/en.json",
    "release": "release-script"
  },
  "bugs": {
    "url": "https://github.com/crycode-de/ioBroker.discord/issues"
  },
  "readmeFilename": "README.md"
}
